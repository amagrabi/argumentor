{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "no-trailing-spaces": "error",
        "no-unused-vars": "warn",
        "no-console": "warn",
        "eqeqeq": "error",
        "curly": "error"
    }
}